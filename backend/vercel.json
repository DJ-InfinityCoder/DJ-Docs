{
    "version" : 2,
    "builds" : [
        {
            "src" : "index.js",
            "use" : "@vercel/node"
        }
    ],
    "routes" : [
        {
            "src" : "/(.*)",
            "dest" : "/",
            "methods": ["GET", "POST", "PUT", "DELETE"], 
            "header" : {
                "Access-Control-Allow-Origin":"*"
            }
        }
    ]
}     